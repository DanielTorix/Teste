<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Forecasted_Fulfilment__c</fullName>
    <description>Forecasted Fulfilment in %</description>
    <externalId>false</externalId>
    <formula>IF(Region__c = 1,
IF(Contracted_Quantity__c =0,0, Forecasted_Quantity__c/Contracted_Quantity__c),
IF(Shifted_Quantity__c =0,0, Forecasted_Quantity__c/Shifted_Quantity__c)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Forecasted Fulfilment in %</inlineHelpText>
    <label>Forecasted Fulfilment</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
