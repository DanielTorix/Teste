<?xml version="1.0" encoding="UTF-8"?>
<MatchingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <matchingRules>
        <fullName>LeadToAccountConversionTraddingMatchingRule</fullName>
        <description>Matching Rule that checks if the Account have the exact Trade Registration Number as the Lead</description>
        <label>Lead to Account Conversion Tradding matching rule</label>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>TradeRegistrationNumber__c</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <ruleStatus>Active</ruleStatus>
    </matchingRules>
    <matchingRules>
        <fullName>LeadToAccountConversionVATMatchingRule</fullName>
        <description>Matching Rule that checks if the Account have the exact VAT Number as the Lead</description>
        <label>Lead to Account Conversion VAT matching rule</label>
        <matchingRuleItems>
            <blankValueBehavior>NullNotAllowed</blankValueBehavior>
            <fieldName>VAT_Tax_Number__c</fieldName>
            <matchingMethod>Exact</matchingMethod>
        </matchingRuleItems>
        <ruleStatus>Active</ruleStatus>
    </matchingRules>
</MatchingRules>
