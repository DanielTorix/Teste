<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Valid_To_Date__c</fullName>
    <externalId>false</externalId>
    <formula>IF(VALUE(RIGHT(Valid_to__c, 2)) = 12, 
					DATE(VALUE(LEFT(Valid_to__c, 4)), VALUE(RIGHT(Valid_to__c, 2)), 31),
			DATE(VALUE(LEFT(Valid_to__c, 4)), VALUE(RIGHT(Valid_to__c, 2))+1, 1)-1
			)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Valid To Date</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
