  "customCSS" : ".salesforceIdentityLoginBody2 {\n    background: white !important;\n}\n/*\n.themeHeaderTopRow,.themeHeaderBottomRow{\n    background-image: -webkit-gradient(linear,left top,right top,color-stop(50%,#036),color-stop(70%,#008fd1),color-stop(90%,#036)) !important;\n    background-image: linear-gradient(-270deg,#036 50%,#008fd1 70%,#036 90%) !important;\n}*/\n.themeBgImage{\n    background-image: linear-gradient(0deg, rgba(102,204,0,0) 72%, #ffffff 28%),\n        -webkit-gradient(linear,left top,right top,color-stop(50%,#036),color-stop(70%,#008fd1),color-stop(90%,#036)) !important;\n}\n\n.themeLayoutStarterWrapper{\n   /* background-image: -webkit-gradient(linear,left top,right top,color-stop(50%,#036),color-stop(70%,#008fd1),color-stop(90%,#036)) !important;\n    background-image: linear-gradient(-270deg,#036 50%,#008fd1 70%,#036 90%) !important;\n    */\n    background: white !important;\n}\n/* HEADER */\n.themeHeaderInner{\n    display:grid !important;\n    grid-template-columns: 2 fr;\n    grid-auto-columns: 50%;\n    grid-auto-flow: column;\n    align-items:center;\n    padding-left:0px !important;\n    padding-top:20px !important;\n    height:140px !important;\n    margin-top:25px !important;\n    margin-bottom:15px !important;\n}\n\n.themeBgImage{\n\tborder-bottom: .3125rem solid rgb(102,204,0);\n}\n\n.bgImage {\n    margin-top: -16px !important;\n}\n\n.bgOverlay {\n    margin-top: -16px !important;\n}\n\n.themeUtilsContainer{\n    justify-content: flex-end !important;\n}\n.themeNav{\n    width:100% !important;\n    grid-column:1 !important;\n\tgrid-row:2 !important;\n}\n\n.themeNav .ui-widget{\n    margin-top:-80px !important;\n    font-size: 20px;\n}\n.themeSearch{\n    grid-column:1 !important;\n\tgrid-row:2 !important;\n    margin-top:-27px !important;        \n    margin-bottom:100px !important;\n    margin-left:-95% !important;\n    margin-right: 15% !important;\n    border-style: solid !important;\n    border-width: 1px !important;\n    border-radius: 18px !important; \n    max-width: 50% !important;\n}\n\n.search-box{\n    border-style: none !important;\n    border-width: 1px !important;\n    border-radius: 18px !important; \n    max-width: 100% !important;\n    padding-right:99% !important;\n}\n\n.themeLogo{\n    flex-grow:0 !important;\n    margin-left: auto !important; \n\tmargin-right: 0px !important;\n    grid-column:2 !important;\n\tgrid-row:2 !important;\n    margin-bottom:-20px !important;\n    margin-top:-100px !important; \n}\n\n.logoImage{\n    width:70px !important;\n    margin-top:-60% !important;\n    margin-right:5% !important;\n}\n.themeUtils{\n    justify-content: flex-start !important;\n    grid-column:2 !important;\n\tgrid-row:1 !important;\n    padding-left: 0 !important;\n    margin-left: 10% !important;\n    margin-bottom: 22px !important;\n \tmargin-top:-10px !important;\n    margin-right: -25% !important;\n}\n.themeProfileMenu{\n    justify-content: flex-end !important;\n    margin-left: 100% !important;\n    margin-right: 15% !important;\n    margin-bottom:100px !important;\n    margin-top:-25px !important;\n    padding-bottom:5px;\n    white-space: nowrap !important;\n    padding-left: 10% !important;\n    padding-right: 10% !important;\n    word-break: keep-all !important;\n    max-width: 100% !important;\n}\n\n .comm-user-profile-menu__login-button{\n    white-space: nowrap !important;\n    margin-left:20% !important;\n    margin-right: -40% !important;\n    word-break: keep-all !important;\n    max-width: 100% !important;\n     padding-left: 40% !important;\n}\n\n.comm-user-profile-menu__trigger-p{\n  color: black !important;\n  white-space: nowrap !important;\n  word-break: keep-all !important;max-width: 100% !important;\n  max-width: 100% !important;\n  margin-right: -20% !important;\n}\n\n.comm-user-profile-menu__trigger-p:hover{\n    color: rgb(105, 105, 105) !important;\n}\n\n/* HEADER */\n\n.footer, .cFooterPanel{\n    background-color: white ;\n}\n.forceActionButton:first-child{\n    box-shadow: 0 0 10px 0 #888888 !important;\n    background-color:white !important;\n    color:black !important;\n}\n.forceActionButton{\n     box-shadow: 0 0 10px 0 #888888 !important;\n    background-color:#33CC00 !important;\n    color:white !important;\n    border:#33CC00 !important;\n}\n.loginButton{\n    background-color:#33CC00 !important;\n    color:white !important;\n     box-shadow: 0 0 10px 0 #888888 !important;\n     border:#33CC00 !important;\n}\n.inverseTextColor{\n     color:#036 !important;\n    \n}\n.inverseTextColor:hover {\n  color: #0099ff !important;\n    text-decoration: underline !important;\n}\n.data-cancelButton{\n     box-shadow: 0 0 10px 0 #888888 ;\n    background-color:white !important;\n    color:black !important ;\n}\n.saveButton{\n     background-color:#33CC00 !important;\n    color:white !important;\n     box-shadow: 0 0 10px 0 #888888 !important;\n     border:#33CC00 !important;\n}\n.forceActionLink{\n     background-color:#33CC00 !important;\n    color:white !important;\n     box-shadow: 0 0 10px 0 #888888 !important;\n    border:#33CC00 !important;\n}\n\n.btn {\n  border: none;\n  color: #33CC00;\n  padding: 12px 16px; \n  font-size: 16px;\n  cursor: pointer; \n  position:absolute !important;\n  top:-10px !important;\n}\n\n.btn:hover {\n  background-color: Blue;\n}\n\nbutton {\n    color: white;\n    background-color: #33cc00;\n    text-align: center;\n    border: none;\n    font-size: 14px;\n    border-radius: 30px;\n    padding: 1.5%;\n    text-decoration: none;\n    display: inline-block;\n    margin: 2% 0%;\n    cursor: pointer;\n    margin-left:3%;\n}\n\n.BulletPoint ul {\n  list-style: none;\n}\n\n.BulletPoint ul li::before {\n  content: \"\\2022\";\n  color: #33CC00;\n  font-weight: bold;\n  display: inline-block; \n  width: 1em;\n  margin-left: -1em;\n}\n\n.HeaderImageIndex{\n    width:100%;\n    margin-left:-40px !important;\n}\n\nHero {\n    margin-top:300px !important;\n}\n\ntable tr {\n    border-bottom:1px solid #EAECEE;\n    }\n\n.salesforceIdentityLoginForm2 .uiButton {\n    margin-left: 0% !important;\n}",
