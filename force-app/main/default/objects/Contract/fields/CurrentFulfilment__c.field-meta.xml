<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CurrentFulfilment__c</fullName>
    <description>Current Fulfilment formula calculated in Salesforce</description>
    <externalId>false</externalId>
    <formula>IF(ShiftedQuantity__c = 0 ,0, LiftedQuantity__c / ShiftedQuantity__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Current Fulfilment</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
